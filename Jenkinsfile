pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo "Build"
			}
		}
	}
               stage('Test') {
                        steps {
                                echo "Build"
                                echo "Test"
                        }
                }
        }
               stage('Integration Test') {
                        steps {
                                echo "Build"
                                echo "Test"
                                echo "Integration Test"
                  }
         }
}
